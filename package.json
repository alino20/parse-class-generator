{
  "name": "parse-class-generator",
  "version": "0.2.3",
  "description": "Tool to generate Typescript class declarations automatically from schema",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean && npm run copy",
    "build": "tsc",
    "test": "mocha",
    "clean": "rimraf ./dist",
    "copy": "copyfiles -u 1 ./src/types.d.ts ./dist/",
    "publish": "semantic-release"
  },
  "keywords": [
    "parse",
    "typescript"
  ],
  "author": "Ali Nosrati",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^22.7.4",
    "parse": "^5.3.0",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/mocha": "^10.0.8",
    "@types/parse": "^3.0.9",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.12.0",
    "fast-check": "^3.22.0",
    "globals": "^15.10.0",
    "mocha": "^10.7.3",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
