{
  "name": "parse-class-generator",
  "version": "0.2.3",
  "description": "Tool to generate Typescript class declarations automatically from schema",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run clean && npm run copy",
    "build": "tsc",
    "test": "mocha",
    "clean": "rimraf ./dist",
    "copy": "copyfiles -u 1 ./src/types.d.ts ./dist/",
    "publish": "semantic-release"
  },
  "keywords": [
    "parse",
    "typescript"
  ],
  "author": "Ali Nosrati",
  "license": "ISC",
  "peerDependencies": {
    "parse": "^5.3.0"
  },
  "dependencies": {
    "@types/node": "^22.15.2",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/mocha": "^10.0.10",
    "@types/parse": "^3.0.9",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.1",
    "fast-check": "^4.1.1",
    "globals": "^16.0.0",
    "mocha": "^11.1.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*",
    "README.md"
  ]
}
